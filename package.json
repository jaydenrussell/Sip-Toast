{
  "name": "sip-toast",
  "version": "0.72.36",
  "buildDate": "2026-02-18T07:47:31.377Z",
  "description": "Windows 11-ready SIP caller ID toast that looks up clients via Acuity Scheduling.",
  "main": "src/main/main.js",
  "author": "Jayden Russell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaydenrussell/Sip-Toast.git"
  },
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "dev": "electronmon .",
    "lint": "eslint \"src/**/*.js\"",
    "package:squirrel": "electron-builder --win --config.win.target=squirrel",
    "create-cert": "powershell -ExecutionPolicy Bypass -File scripts/create-certificate.ps1",
    "release": "node scripts/create-release.js"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "eventemitter3": "^5.0.4",
    "sip": "^0.0.6",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "electronmon": "^2.0.2",
    "eslint": "^9.39.2"
  },
  "build": {
    "appId": "com.yoursolution.siptoast",
    "productName": "SIPToast",
    "files": [
      "src/**/*",
      "Images/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "win": {
      "target": [
        {
          "target": "squirrel",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "Images/app.ico",
      "artifactName": "SIPToast-Setup-${version}.${ext}"
    },
    "squirrelWindows": {
      "name": "SIPToast",
      "iconUrl": "https://raw.githubusercontent.com/jaydenrussell/Sip-Toast/main/Images/app.ico"
    },
    "afterPack": "scripts/optimize-build.js",
    "asar": true,
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "compression": "maximum",
    "removePackageScripts": true,
    "publish": {
      "provider": "github",
      "owner": "jaydenrussell",
      "repo": "Sip-Toast"
    }
  }
}